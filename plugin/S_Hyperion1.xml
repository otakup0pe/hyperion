      <name>A_ARG_TYPE_Increment</name>,
      <dataType>int</dataType>
      <allowedValueRange>
        <minimum>1</minimum>
        <maximum>100</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_GraceTime</name>,
      <dataType>int</dataType>
      <allowedValueRange>
        <minimum>0</minimum>
        <maximum>10800</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Temp</name>,
      <dataType>int</dataType>
      <allowedValueRange>
        <minimum>1500</minimum>
        <maximum>5500</maximum>
