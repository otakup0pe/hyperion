      <name>A_ARG_TYPE_LogLevel</name>,
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue>debug</allowedValue>
        <allowedValue>info</allowedValue>
        <allowedValue>warn</allowedValue>
        <allowedValue>error</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_TempName</name>,
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue>DayTemp</allowedValue>
        <allowedValue>NightTemp</allowedValue>
        <allowedValue>EveningTemp</allowedValue>
        <allowedValue>PresetOne</allowedValue>
        <allowedValue>PresetTwo</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_TimeName</name>,
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue>Morning</allowedValue>
        <allowedValue>Night</allowedValue>
      </allowedValueList>
    </stateVariable>
