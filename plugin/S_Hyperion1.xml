<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_LogLevel</name>,
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue>debug</allowedValue>
        <allowedValue>info</allowedValue>
        <allowedValue>warn</allowedValue>
        <allowedValue>error</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_TempName</name>,
      <dataType>string</dataType>
      <allowedValueList>
        <allowedValue></allowedValue>
        <allowedValue>info</allowedValue>
        <allowedValue>warn</allowedValue>
        <allowedValue>error</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Temp</name>,
      <dataType>int</dataType>
      <allowedValueRange>
        <minimum>1500</minimum>
        <maximum>5500</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>A_ARG_TYPE_Bool</name>,
      <dataType>int</dataType>
      <allowedValueRange>
        <minimum>0</minimum>
        <maximum>1</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>LogLevel</name>
      <dataType>string</dataType>
      <defaultValue>debug</defaultValue>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>NightTime</name>
      <dataType>int</dataType>
      <defaultValue>22</defaultValue>
      <allowedValueRange>
        <minimum>0</minimum>
        <maximum>23</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>MorningTime</name>
      <dataType>int</dataType>
      <defaultValue>06</defaultValue>
      <allowedValueRange>
        <minimum>0</minimum>
        <maximum>23</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>SunsetGrace</name>
      <dataType>int</dataType>
      <defaultValue>900</defaultValue>
      <allowedValueRange>
        <minimum>0</minimum>
        <maximum>3600</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>SunriseGrace</name>
      <dataType>int</dataType>
      <defaultValue>1800</defaultValue>
      <allowedValueRange>
        <minimum>0</minimum>
        <maximum>3600</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>NightTemp</name>
      <dataType>int</dataType>
      <defaultValue>1500</defaultValue>
      <allowedValueRange>
        <minimum>1500</minimum>
        <maximum>5500</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>EveningTemp</name>
      <dataType>int</dataType>
      <defaultValue>4500</defaultValue>
      <allowedValueRange>
        <minimum>1500</minimum>
        <maximum>5500</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>PresetTemp</name>
      <dataType>int</dataType>
      <defaultValue>3250</defaultValue>
      <allowedValueRange>
        <minimum>1500</minimum>
        <maximum>5500</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>DayTemp</name>
      <dataType>int</dataType>
      <defaultValue>5500</defaultValue>
      <allowedValueRange>
        <minimum>1500</minimum>
        <maximum>5500</maximum>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>Ambience</name>
      <dataType>int</dataType>
      <defaultValue>1</defaultValue>
      <allowedValueRange>
        <minimum>0</minimum>
        <maximum>1</maximum>
      </allowedValueRange>
    </stateVariable>
  </serviceStateTable>
  <actionList>
    <action>
      <name>SetLogLevel</name>
      <argumentList>
        <argument>
          <name>Level</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Level</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetTemp</name>
      <argumentList>
        <argument>
          <name>Name</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_TempName</relatedStateVariable>
        </argument>
        <argument>
          <name>Temp</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Temp</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
    <action>
      <name>SetAmbience</name>
      <argumentList>
        <argument>
          <name>Enabled</name>
          <direction>in</direction>
          <relatedStateVariable>A_ARG_TYPE_Bool</relatedStateVariable>
        </argument>
      </argumentList>
    </action>
  </actionList>
</scpd>
